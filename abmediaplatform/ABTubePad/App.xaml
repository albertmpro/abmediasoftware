<Application x:Class="ABTubePad.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ABTubePad"
             StartupUri="View/MainView.xaml">
    <Application.Resources>
    <ResourceDictionary>
         <ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Resources/Default.xaml" />
				<ResourceDictionary Source="/Resources/UI.xaml" />
				<ResourceDictionary Source="/Resources/DataTemp.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
    </Application.Resources>
</Application>
